global:
route:
  group_by: ['alertname']
  group_wait: 30s
  group_interval: 10m
  repeat_interval: 60m
  receiver: 'email'
receivers:
- name: 'email'
  email_configs:
  - to: 'test@ovirt.ru'
    from: 'test@ovirt.ru'
    smarthost: 'ovirt.ru:25'
    auth_username: 'user'
    auth_identity: 'user'
    auth_password: 'user'
